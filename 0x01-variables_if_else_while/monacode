#include <stdio.h>
/**
 * main - Entry point
 *
 * Description: the program that output all possible
 * combinations of single-digit numbers and print
 *
 *
 * Return: Always 0 (Success)
 **/
int main(void)
{
	int i = 0;
	int k;
	int m;
	int o;

	while (i < 10)
	{
		k = i + 1;
		while (k < 10)
		{
			m = k + 1;
			while (m < 10)
			{
				o = m + 1;
				while (o < 10)
				{
					if (i != k || i < k)
					{
							putchar(i + '0');
							putchar(k + '0');
							putchar(' ');
							putchar(m + '0');
							putchar(o + '0');
							if (i + k + o + m < 35)
							{
							putchar(',');
							putchar(' ');
							}
					}
						o++;
				}
				m++;
			}
		k++;
		}
		i++;
	}
	putchar('\n');
	return (0);
}
